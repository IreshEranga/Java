import java.util.Scanner; //import scanner
public class Main{
    public static void main(String[] args){


        System.out.println("Enter an integer : ");
        Scanner number = new Scanner(System.in);
       int num = number.nextInt();

       for(int i=1;i<=10;i++)
       {
           System.out.println(" "+num+" * "+i+" = "+num*i);
       }
        }
        }
