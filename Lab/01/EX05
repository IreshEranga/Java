public class Main {
    public static void main(String[] args) {
     //   int rows = 5; // Number of rows
       // int cols = 5; // Number of columns

        for (int i = 0; i < 5; i++) {
            for (int j = 0; j < 5; j++) {
                System.out.print("* ");
            }
            System.out.println(); // Move to the next line after printing each row
        }
    }
}
